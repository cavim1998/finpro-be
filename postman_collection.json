            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{accessToken}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "/path/to/image.jpg"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/users/profile/photo",
              "host": ["{{baseUrl}}"],
              "path": ["users", "profile", "photo"]
            }
          }
        },
